{
  "name": "anodio/core",
  "autoload": {
    "psr-4": {
      "Anodio\\Core\\": "src/"
    }
  },
    "autoload-dev": {
        "psr-4": {
            "AnodioCoreTests\\": "tests/"
        }
    },
  "require": {
    "olvlvl/composer-attribute-collector": "v2.0.1",
    "vlucas/phpdotenv": "^5.6",
    "monolog/monolog": "^3.7",
    "symfony/console": "v7.1.3",
    "php-di/php-di": "^7.0"
  },
    "require-dev": {
        "phpunit/phpunit": "^11.3"
    },
  "extra": {
      "composer-attribute-collector": {
          "include": [
              "src",
            "tests/Support"
          ]
      }
  },
  "config": {
    "allow-plugins": {
      "olvlvl/composer-attribute-collector": true
    }
  }
}
